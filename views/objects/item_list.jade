a.clearfix.list-group-item(name='#{objects[object].id}', href='/objects/#{objects[object].id}')
  div.col-sm-6
    h4.list-group-item-heading(style="margin:0; padding:0;")
      span.fa.fa-cube
      | &nbsp;#{objects[object].name}&nbsp;

  div.col-sm-2
    - if(objects[object].latitude && objects[object].longitude)
      span.fa.fa-map(data-toggle='tooltip', data-placement='top', title='Latitude')  #{objects[object].latitude}
      | &nbsp;&nbsp;
      span(data-toggle='tooltip', data-placement='top', title='Longitude') / #{objects[object].longitude}
    - else
    - end
  
  div.col-sm-2
    -if(objects[object].isPublic=='true')
      div.fa.fa-eye  Public
    -else
      div.fa.fa-eye-slash  Private
    -end

  div.col-sm-2
    span.fa.fa-clock-o  #{moment(objects[object].meta.created).format('DD/MM/YYYY, HH:mm')}

  div.col-sm-2
    span.label.label-default.ellipsis(data-toggle='tooltip', data-placement='top', title='Object Id')  #{objects[object].id}
      
  div.col-sm-4
    div &nbsp;

  div.col-sm-4
    -if(objects[object].position)
      div.fa.fa-map-marker(data-toggle='tooltip', data-placement='top', title='Localization')  #{objects[object].position}
    -else
      div &nbsp;
    -end

  div.col-sm-2
    div.fa.fa-clock-o  #{moment(objects[object].meta.updated).format('DD/MM/YYYY, HH:mm')}
  
  div.col-sm-10
    div #{objects[object].description}

  div.col-sm-2
    div.fa.fa-chevron-right.pull-right 
