- var p = 1
- if (page>0)
  - var prevPage = page-1
- if (page<pagenb)
  - var nextPage = prevPage+2
- else
  - var nextPage = ''

nav.clearfix.pull-left.col-xs-8
  ul.pagination.clearfix
    - if (prevPage>0)
      li
        - if ( currentUrl === '/objects/add' )
          a(href='/objects?page=#{prevPage}', aria-label='Previous')
            span(aria-hidden='true', title='Previous') ←

        - else if ( currentUrl === '/flows/add' )
          a(href='/flows?page=#{prevPage}', aria-label='Previous')
            span(aria-hidden='true', title='Previous') ←

        - else if ( currentUrl === '/snippets/add' )
          a(href='/snippets?page=#{prevPage}', aria-label='Previous')
            span(aria-hidden='true', title='Previous') ←

        - else if ( currentUrl === '/dashboards/add' )
          a(href='/dashboards?page=#{prevPage}', aria-label='Previous')
            span(aria-hidden='true', title='Previous') ←

        - else if ( currentUrl === '/keys/add' )
          a(href='/keys?page=#{prevPage}', aria-label='Previous')
            span(aria-hidden='true', title='Previous') ←

        - else
          a(href='?page=#{prevPage}', aria-label='Previous')
            span(aria-hidden='true', title='Previous') ←

    - while (p <= pagenb)
      - if (p == page)
        li.active
          a #{p}
      - else
        li
          - if ( currentUrl === '/objects/add' )
            a(href="/objects?page=#{p}") #{p}

          - else if ( currentUrl === '/flows/add' )
            a(href="/flows?page=#{p}") #{p}

          - else if ( currentUrl === '/snippets/add' )
            a(href="/snippets?page=#{p}") #{p}

          - else if ( currentUrl === '/dashboards/add' )
            a(href="/dashboards?page=#{p}") #{p}

          - else if ( currentUrl === '/keys/add' )
            a(href="/keys?page=#{p}") #{p}

          - else
            a(href="?page=#{p}") #{p}

      - var p=(p+1)
    - if (nextPage)
      li
        - if ( currentUrl === '/objects/add' )
          a(href='/objects?page=#{nextPage}', aria-label='Next')
            span(aria-hidden='true', title='Next') →

        - else if ( currentUrl === '/flows/add' )
          a(href='/flows?page=#{nextPage}', aria-label='Next')
            span(aria-hidden='true', title='Next') →

        - else if ( currentUrl === '/snippets/add' )
          a(href='/snippets?page=#{nextPage}', aria-label='Next')
            span(aria-hidden='true', title='Next') →

        - else if ( currentUrl === '/dashboards/add' )
          a(href='/dashboards?page=#{nextPage}', aria-label='Next')
            span(aria-hidden='true', title='Next') →

        - else if ( currentUrl === '/keys/add' )
          a(href='/keys?page=#{nextPage}', aria-label='Next')
            span(aria-hidden='true', title='Next') →

        - else
          a(href='?page=#{nextPage}', aria-label='Next')
            span(aria-hidden='true', title='Next') →

    li.separator &nbsp;

nav.clearfix.pull-right.col-xs-4
  ul.list-unstyled.clearfix.pull-right
    li
      br/
      - if ( currentUrl === '/objects' || currentUrl === '/objects/' )
        - if( objects_length < quota.objects )
          a.btn.btn-primary.m-t(href="/objects/add")
            span.fa.fa-plus  Add Object
        - else
          a.btn.btn-danger.m-t(href="/plans")
            span.fa.fa-plus  Upgrade your plan
              br/
              span to add an Object

      - else if ( currentUrl === '/flows' || currentUrl === '/flows/' )
        - if( flows_length < quota.flows )
          a.btn.btn-primary.m-t(href="/flows/add")
            span.fa.fa-plus  Add Flow
        - else
          a.btn.btn-danger.m-t(href="/plans")
            span.fa.fa-plus  Upgrade your plan
              br/
              span to add a Flow

      - else if ( currentUrl === '/snippets' || currentUrl === '/snippets/' )
        - if( snippets_length < quota.snippets )
          a.btn.btn-primary.m-t(href="/snippets/add")
            span.fa.fa-plus  Add Snippet
        - else
          a.btn.btn-danger.m-t(href="/plans")
            span.fa.fa-plus  Upgrade your plan
              br/
              span to add a Snippet

      - else if ( currentUrl === '/dashboards' || currentUrl === '/dashboards/' )
        - if( dashboards_length < quota.dashboards )
          a.btn.btn-primary.m-t(href="/dashboards/add")
            span.fa.fa-plus  Add Dashboard
        - else
          a.btn.btn-danger.m-t(href="/plans")
            span.fa.fa-plus  Upgrade your plan
              br/
              span to add a Dashboard

      - else if ( currentUrl === '/keys' || currentUrl === '/keys/' )
        - if( tokens_length < quota.tokens )
          a.btn.btn-primary.m-t(href="/keys/add")
            span.fa.fa-plus  Add an API Key
        - else
          a.btn.btn-danger.m-t(href="/plans")
            span.fa.fa-plus  Upgrade your plan
              br/
              span to add an API Key

